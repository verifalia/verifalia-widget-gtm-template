___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');

log("data=", data);

// Widget configuration

const widgetConfig = copyFromWindow('VerifaliaWidgetConfig') || {};

widgetConfig.appKey = data.appKey;

if (data.advancedSettings) {
  for (let settingIdx = 0; settingIdx < data.advancedSettings.length; settingIdx++) {
    const keyParts = data.advancedSettings[settingIdx].key.split('.');
    
    // Retrieve the target settings object, creating any missing entries if necessary
    
    let target = widgetConfig;
    
    for (let keyPartIdx = 0; keyPartIdx < (keyParts.length - 1); keyPartIdx++) {
      target[keyParts[keyPartIdx]] = target[keyParts[keyPartIdx]] || {};
      target = target[keyParts[keyPartIdx]];
    }
    
    // Set the target property
    
    target[keyParts[keyParts.length - 1]] = data.advancedSettings[settingIdx].value;
  }
}

log("widgetConfig=", widgetConfig);

setInWindow('VerifaliaWidgetConfig', widgetConfig, true);

// Script injection

injectScript(
  'https://unpkg.com/verifalia-widget@1.12.1/dist/verifalia-widget.js',
  function() {
    data.gtmOnSuccess();
  },
  function() {
    data.gtmOnFailure();
  }
);


